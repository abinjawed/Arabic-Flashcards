<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/alif"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/alifnormal"
        />

    <Button
        android:id="@+id/ba"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="120dp"
        app:layout_constraintEnd_toStartOf="@+id/alif"
        app:layout_constraintStart_toEndOf="@+id/taa"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/banormal"
        />

    <Button
        android:id="@+id/taa"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="120dp"
        app:layout_constraintStart_toEndOf="@+id/tha"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/tanormal"
        />

    <Button
        android:id="@+id/tha"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="120dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/thanormal"
        />

    <Button
        android:id="@+id/jeem"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alif"
        android:background="@drawable/jeemnormal"
        />

    <Button
        android:id="@+id/h6a"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toStartOf="@+id/jeem"
        app:layout_constraintStart_toEndOf="@+id/kha"
        app:layout_constraintTop_toBottomOf="@+id/ba"
        android:background="@drawable/hhanormal"
        />

    <Button
        android:id="@+id/kha"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@+id/dal"
        app:layout_constraintTop_toBottomOf="@+id/taa"
        android:background="@drawable/khanormal"
        />

    <Button
        android:id="@+id/dal"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tha"
        android:background="@drawable/danormal"
        />

    <Button
        android:id="@+id/thal"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jeem"
        android:background="@drawable/thalnormal"
        />

    <Button
        android:id="@+id/ra"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/thal"
        app:layout_constraintStart_toEndOf="@+id/zay"
        app:layout_constraintTop_toBottomOf="@+id/h6a"
        android:background="@drawable/ranormal"
        />

    <Button
        android:id="@+id/zay"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@+id/seen"
        app:layout_constraintTop_toBottomOf="@+id/kha"
        android:background="@drawable/zednormal"
        />

    <Button
        android:id="@+id/seen"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dal"
        android:background="@drawable/seennormal"
        />

    <Button
        android:id="@+id/sheen"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thal"
        android:background="@drawable/sheennormal"
        />

    <Button
        android:id="@+id/s9aad"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/sheen"
        app:layout_constraintTop_toBottomOf="@+id/ra"
        android:background="@drawable/sadnormal"
        />

    <Button
        android:id="@+id/dhaad"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/s9aad"
        app:layout_constraintTop_toBottomOf="@+id/zay"
        android:background="@drawable/dhadnormal"
        />

    <Button
        android:id="@+id/t6a"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/dhaad"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seen"
        android:background="@drawable/t6anormal"
        />

    <Button
        android:id="@+id/zha"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sheen"
        android:background="@drawable/zhanormal"
        />

    <Button
        android:id="@+id/a3ayn"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/zha"
        app:layout_constraintTop_toBottomOf="@+id/s9aad"
        android:background="@drawable/a3aynnormal"
        />

    <Button
        android:id="@+id/ghayn"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/a3ayn"
        app:layout_constraintTop_toBottomOf="@+id/dhaad"
        android:background="@drawable/ghaynnormal"
        />

    <Button
        android:id="@+id/fa"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/ghayn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t6a"
        android:background="@drawable/fanormal"
        />

    <Button
        android:id="@+id/qaf"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/zha"
        android:background="@drawable/qafnormal"
        />

    <Button
        android:id="@+id/kaf"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/qaf"
        app:layout_constraintTop_toBottomOf="@+id/a3ayn"
        android:background="@drawable/kafnormal"
        />

    <Button
        android:id="@+id/lam"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/kaf"
        app:layout_constraintTop_toBottomOf="@+id/ghayn"
        android:background="@drawable/lamnormal"
        />

    <Button
        android:id="@+id/meem"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/lam"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fa"
        android:background="@drawable/meemnormal"
        />

    <Button
        android:id="@+id/noon"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qaf"
        android:background="@drawable/noonnormal"
        />

    <Button
        android:id="@+id/ha"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/noon"
        app:layout_constraintTop_toBottomOf="@+id/kaf"
        android:background="@drawable/hanormal"
        />

    <Button
        android:id="@+id/wow"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/ha"
        app:layout_constraintTop_toBottomOf="@+id/lam"
        android:background="@drawable/wawnormal"
        />

    <Button
        android:id="@+id/ya"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toStartOf="@+id/wow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/meem"
        android:background="@drawable/yanormal"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
